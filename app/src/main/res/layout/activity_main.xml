<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:focusedByDefault="true">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="160dp"
        android:layout_marginTop="100dp"
        android:elegantTextHeight="false"
        android:text="Hello!"
        android:textAllCaps="true"
        android:textSize="24sp"
        tools:ignore="HardcodedText" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="70dp"
        android:layout_marginTop="140dp"
        android:text="How are you feeling today?"
        android:textSize="24sp" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="90dp"
        android:layout_height="92dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="200dp"
        android:scaleType="center"
        app:srcCompat="@drawable/screenshot_2023_04_04_215054"
        tools:ignore="ContentDescription,SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="94dp"
        android:layout_height="94dp"
        android:layout_marginStart="250dp"
        android:layout_marginTop="200dp"
        app:srcCompat="@drawable/screenshot_2023_04_04_220849"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="95dp"
        android:layout_height="94dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="320dp"
        app:srcCompat="@drawable/screenshot_2023_04_04_215917"
        tools:ignore="ContentDescription,SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="93dp"
        android:layout_height="90dp"
        android:layout_marginStart="250dp"
        android:layout_marginTop="320dp"
        app:srcCompat="@drawable/screenshot_2023_04_04_220929"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="99dp"
        android:layout_height="90dp"
        android:layout_marginStart="150dp"
        android:layout_marginTop="450dp"
        app:srcCompat="@drawable/screenshot_2023_04_04_220944"
        tools:ignore="SpeakableTextPresentCheck" />

</RelativeLayout>